<Script_VL SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
VALUES "typ1" "Kör"
VALUES "typ2" "Tompa", "Feles", "Hegyes"

IF kfix THEN
	VALUES "xArrow" kd/2, kf
else
	hideparameter "kf"
ENDIF

for i = 1 to vardim1(xArrow)
	IF xArrow[i] < kd/2 + EPS THEN xArrow[i] = kd/2
next i

PARAMETERS xArrow=xArrow


]]>
</Script_VL>

