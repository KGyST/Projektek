<Script_3D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
resol gs_resol
material gs_tap_mat

mul 1, -1, 1
	hotspot 0, 0, 0
	
	hotspot 0, yShower, 			0,				unID, mountingHeight, 	1	 + 128	: unID = unID + 1
	hotspot 0, yShower, 			mountingHeight,	unID, mountingHeight, 	2 			: unID = unID + 1
	hotspot 0, yShower, 			-1,				unID, mountingHeight, 	3			: unID = unID + 1
	
	hotspot 0,	0,					mountingHeight,	unID, yShower, 			1 + 128		: unID = unID + 1
	hotspot 0,	yShower,			mountingHeight,	unID, yShower, 			2			: unID = unID + 1
	hotspot 0,	-1,					mountingHeight,	unID, yShower, 			3			: unID = unID + 1
	
	hotspot 0,	yShower,			mountingHeight - 0.016,	unID, rOuter, 1	: unID = unID + 1
	hotspot 0,	yShower + rOuter,	mountingHeight - 0.016,	unID, rOuter, 2	: unID = unID + 1
	hotspot 0,	yShower-1,			mountingHeight - 0.016,	unID, rOuter, 3	: unID = unID + 1

	if TapSymbol_m = TAPSYMBOL_DETAILED then
		hotspot 0,	yShower,			mountingHeight - 0.013,	unID, rInner, 1	: unID = unID + 1
		hotspot 0,	yShower + rInner,	mountingHeight - 0.013,	unID, rInner, 2	: unID = unID + 1
		hotspot 0,	yShower-1,			mountingHeight - 0.013,	unID, rInner, 3	: unID = unID + 1
	endif

	addz mountingHeight
		if gs_tap_type_m = 24 then
			hotspot rOverhang,		yShower,				mountingHeight,	unID, yOverhang, 1 + 128	: unID = unID + 1
			hotspot rOverhang,		yShower + yOverhang,	mountingHeight,	unID, yOverhang, 2			: unID = unID + 1
			hotspot rOverhang,		yShower-1,				mountingHeight,	unID, yOverhang, 3			: unID = unID + 1
			
			hotspot 0,				yShower + yOverhang,	mountingHeight,	unID, rOverhang, 1			: unID = unID + 1
			hotspot rOverhang,		yShower + yOverhang,	mountingHeight,	unID, rOverhang, 2			: unID = unID + 1
			hotspot -1,				yShower + yOverhang,	mountingHeight,	unID, rOverhang, 3			: unID = unID + 1

			rotz 90
			revolve 4, 360, 1+2+4+8+16+32,
				0, 0.04, 0,
				0.008, 0.04, 0,
				0.008, rOverhang, 0,
				yShower + 0.022, rOverhang, 0
			del 1
			
			rotx 90
			cutpolya 2, 1, 0,
				0, 1.03, 907 +64,
				rOverhang, 360, 4007 + 64
			del 1
		endif
	
		add 0, yShower, 0
		roty 90
			revolve 8, 360, 1+2+4+8+16+32,
						0,		   	0.011,		   0,
					0.013,		   	0.011,		   0,
					0.013,		   	rInner,		   0,
					0.016,		   	rInner,		   0,
					0.016,		   	rOuter,		   0,
					0.028,		   	rOuter,		   0,
					0.028,		   	0.11,		   0,
					0.03,		   	0.11,		   0
		del 2
		
		cutend
	del 1
del 1

! ==============================================================================
! ==============================================================================
end
! ==============================================================================
! ==============================================================================


! ==============================================================================
! Shower Kit
! ==============================================================================
"shower kit for wallmounted taps":
	roundOff1 = 0.002
	roundOff2 = 0.007
	yTubePos = - 0.058*0
	beta = 0
	
	for hi = 1 to 2*gs_resol
	alfa = hi * 180/gs_resol/2
	put yShowerOpenerPos + (yShowerPos)/2-(yShowerPos)/2*cos(alfa), yShowerOpenerPos + yTubePos + ((yShowerPos)/2-(yShowerPos)/2*cos(alfa))*tan(beta), -0.135-(yShowerPos)/2*sin(alfa), 0
	next hi
	
	yTube = yShowerHeadPos + 0.035 - yShowerOpenerPos

	add yShowerOpenerPos + yShowerPos + yShowerHeadPos, yShowerHeadPos + yWallPos, zShowerPos + 0.011
	rotx showerHeadAngle
		addz -0.015
		cylind 0.035, 0.0085
		del 1
	del 2
	
	group "ShowerHead1"
	add yShowerOpenerPos + yShowerPos + yShowerHeadPos, yShowerHeadPos + yWallPos, zShowerPos + 0.026 - 0.015
	rotx showerHeadAngle
		sweepRX = 0.01
		sweepRY = 0.011
		addz 0.02
		sweep 9, 4, 0, 1.05, 1+2+4+16+32,
			sweepRX, 0, 1,
			sweepRX, sweepRY-roundOff2, 1,
			sweepRX-roundOff2, sweepRY, 1001,
		   -sweepRX+roundOff2, sweepRY, 1,
		   -sweepRX, sweepRY-roundOff2, 1001,
		   -sweepRX, -sweepRY+roundOff2, 1,
		   -sweepRX+roundOff2, -sweepRY, 1001,
			sweepRX-roundOff2, -sweepRY, 1,
			sweepRX, -sweepRY+roundOff2, 1001,

		   	0,		   		0,		   0,
		   	0,		  	-0.002,		   0.096,
		   	0,		  	-0.005,		   0.134,
		   	0,		  	-0.007,		   0.152
		del 1
	del 2
	endgroup
	
	group "ShowerHead2"
	add yShowerOpenerPos + yShowerPos + yShowerHeadPos, yShowerHeadPos + yWallPos, zShowerPos + 0.026 - 0.015
	rotx showerHeadAngle
		add 0, 0.017, 0.19+0.02
		rotz -90
		roty 10
			revolve 5, 360, 1+2+4+8+16+32,
				0.048,		   	0.045,		   0,
				0.045,		   	0.045,		   0,
				0.045,		   	rInner,		   0,
				0.018,		   	rInner,		   2,
				0.012,		   	0.03,		   0
		del 3
	del 2
	endgroup
	
	placegroup addgroup ("ShowerHead1","ShowerHead2")
	killgroup "ShowerHead1"
	killgroup "ShowerHead2"
	
	add yShowerOpenerPos + yShowerPos + yShowerHeadPos, yShowerHeadPos + yWallPos, zShowerPos + 0.011
	rotx showerHeadAngle
		cutpolya 2, 1, 0,
			0, 0, 907 + 64,
			0.01, 360, 4007 + 64
		cutpolya 4, 1, 0,
			-0.006, 0, 7,
			-0.006, -0.1, 7,
			 0.006, -0.1, 7,
			 0.006, 0, 7
	del 2
		cutpolya 2, 1, 0,
			yShowerHolderPos, yShowerHeadPos, 915 + 64,
			pipeRadius, 360, 4015 + 64
		
			add yShowerHolderPos - 0.05, yShowerHeadPos, zShowerPos + 0.011
			roty 90
				cylind 0.1, 0.018
			del 2
		cutend
	cutend
	cutend
return

]]>
</Script_3D>

<!-- GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT -->

<Script_2D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
mul2 1, -1
	hotspot2 0,				0,						unID, yShower, 1	: unID = unID + 1
	hotspot2 0,				yShower,				unID, yShower, 2	: unID = unID + 1
	hotspot2 0,				-1,						unID, yShower, 3	: unID = unID + 1
	
	hotspot2 0,				yShower,				unID, rOuter, 1		: unID = unID + 1
	hotspot2 0,				yShower + rOuter,		unID, rOuter, 2		: unID = unID + 1
	hotspot2 0,				yShower-1,				unID, rOuter, 3		: unID = unID + 1
	
if TapSymbol_m = TAPSYMBOL_DETAILED then
	hotspot2 0,				yShower,				unID, rInner, 1		: unID = unID + 1
	hotspot2 0,				yShower + rInner,		unID, rInner, 2		: unID = unID + 1
	hotspot2 0,				yShower-1,				unID, rInner, 3		: unID = unID + 1
endif
	
	! ==============================================================================
	! Wall Mounted Tap - Shower Pipe
	! ==============================================================================
	
	if gs_tap_type_m = 24 then
		if TapSymbol_m = TAPSYMBOL_DETAILED then
			hotspot2 rOverhang,		yShower,				unID, yOverhang, 1 + 128	: unID = unID + 1
			hotspot2 rOverhang,		yShower + yOverhang,	unID, yOverhang, 2			: unID = unID + 1
			hotspot2 rOverhang,		yShower-1,				unID, yOverhang, 3			: unID = unID + 1
			
			hotspot2 0,				yShower + yOverhang,	unID, rOverhang, 1			: unID = unID + 1
			hotspot2 rOverhang,		yShower + yOverhang,	unID, rOverhang, 2			: unID = unID + 1
			hotspot2 -1,			yShower + yOverhang,	unID, rOverhang, 3			: unID = unID + 1

			line2          -rOverhang, 	0,       				rOverhang,       0
			line2          -rOverhang,  0.00,       		   -rOverhang,       yShower + yOverhang
			line2           rOverhang,  0.00,        			rOverhang,       yShower + yOverhang
			line2          -rOverhang, 	yShower + yOverhang,   	rOverhang,       yShower + yOverhang

			aInner = asn(rOverhang / rInner)
			aOuter = asn(rOverhang / rOuter)
	
			arc2     0,		yShower,	rInner, 270 + aInner, 270 - aInner 
			arc2     0,     yShower,    rOuter, 270 + aOuter, 270 - aOuter		!!TODO angle
		else
			circle2	 0,     yShower,    rOuter
		endif
	endif
	
	if gs_tap_type_m = 26 then
		if TapSymbol_m = TAPSYMBOL_DETAILED then
			circle2 0, yShower, rInner
			circle2 0, yShower, rOuter
		else
			circle2 0, yShower, rOuter
		endif
	endif
del 1
]]>
</Script_2D>

