<Script_VL SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
values "gs_tap_type" `Style 1`, `Style 3`				!!, `Style 2`, `Style 4`
values "gs_tap_type_m" 24, 26							!!, 25, 27

values "yShower" range (0, )
values "rInner" range (0, )
values "rOuter" range (rInner, )
values "yOverhang" range (0, rInner]
values "rOverhang" range (0, sqr(rInner * rInner - yOverhang * yOverhang)]

if gs_tap_type_m = 26 then
	hideparameter "yOverhang", "rOverhang"
endif

!!values "TapSymbol" 

if GLOB_MODPAR_NAME = "gs_tap_type" then
	if gs_tap_type = `Style 1` then
		gs_tap_type_m = 24
	endif

	if gs_tap_type = `Style 2` then
		gs_tap_type_m = 25
	endif

	if gs_tap_type = `Style 3` then
		gs_tap_type_m = 26
	endif

	if gs_tap_type = `Style 4` then
		gs_tap_type_m = 27
	endif
endif

parameters gs_tap_type_m  = gs_tap_type_m 
]]>
</Script_VL>

